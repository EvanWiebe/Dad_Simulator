//HOME PROFILE
void State::read_63(char *c) {
    int len;
    dad.getName() = _get_tilde_terminated_string(c);        
    len = dad.getName().length();
    c += len + 1;
    dad.getPicture() = _get_tilde_terminated_string(c);        
    len = dad.getPicture().length();
    c += len + 1;
    dad.getAesthetic() = _get_tilde_terminated_string(c);        
    len = dad.getAesthetic().length();
    c += len + 1;
    dad.getArchetype() = _get_tilde_terminated_string(c);        
    len = dad.getArchetype().length();
    c += len + 1;
